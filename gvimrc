:colorscheme ir_black

" Font
set guifont=Monaco:h10.00

" No audible bell
set vb

" No toolbar
set guioptions-=T

" set lines=999
" set columns=999

" Highlight todo and fixme
highlight Todo          guifg=Blue  guibg=Yellow    gui=NONE
highlight Fixme         guifg=Blue  guibg=Yellow    gui=NONE

