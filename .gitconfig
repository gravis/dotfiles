[github]
  user = gravis
[alias]
  ci = commit
  st = status
  co = checkout
  br = branch
  df = diff
  lg = log -p
  log-graph = log --graph --all --decorate
  last = log --oneline -n 1
[ignore]
  tuneup
[color]
  diff = auto
  status = auto
  branch = auto
  grep = auto
[user]
  name = Philippe LafoucrieÌ€re
  email = philippe.lafoucriere@tech-angels.com
[push]
  default = current
[core]
  excludesfile = ~/.gitignore
[branch]
  autosetuprebase = always
